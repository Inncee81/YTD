				<regexp id="item" match="CONTENT"><![CDATA[swf\s*=\s*new\s+SWFObject\s*\((?P<CONTENT>.+?)\breturn\s+swf\s*;]]></regexp>
				<regexp id="params" match="CONTENT"><![CDATA[\[\s*\[\s*"params"\s*,\s*"(?P<CONTENT>.+?)"]]></regexp>
				<regexp id="url" match="URL"><![CDATA["(?P<QUALITY>\w+)_src"\s*:\s*"(?P<URL>https?:.+?)"]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
