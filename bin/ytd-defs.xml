<ytd-defs>
	<version>1.46.20150315.0</version>
	<scripts>

		<script id="1tv.ru" provider="1tv.ru">
			<regexps>
				<regexp id="playlist" match="CONTENT"><![CDATA[\bplaylistObj\s*=\s*\[(?P<CONTENT>.+?)\]\s*;]]></regexp>
				<regexp id="movie" match="URL"><![CDATA[\{[^}]*'file'\s*:\s*'(?P<URL>https?://[^']+)'\s*,\s*'title'\s*:\s*'(?P<TITLE>[^']*)'[^}]*\}]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="ansi"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="playlist"><regexp id="playlist" content="page"/></set_var>
			<multi_regexp id="movie" content="playlist" var_prefix="movie_" match="URL,TITLE">
				<http_download>
					<title><get_var id="movie_TITLE"/></title>
					<url><get_var id="movie_URL"/></url>
				</http_download>
			</multi_regexp>
		</script>

		<script id="abc.net.au" provider="ABC.net.au">
			<regexps>
				<regexp id="info" match="TITLE,URL"><![CDATA[<h3>\s*(?P<TITLE>[^<]+?)\s*</h3>(?:(?!<h3>).)*?<a\s+href="(?P<URL>https?://[^"]+\.mp4)"]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="ansi"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<multi_regexp id="info" content="page" var_prefix="info_" match="URL,TITLE">
				<http_download>
					<title><get_var id="info_TITLE"/></title>
					<url><get_var id="info_URL"/></url>
				</http_download>
			</multi_regexp>
		</script>

		<script id="aktualne.cz" provider="Aktualne.cz">
			<regexps>
				<regexp id="url_bitrate" match="BITRATE"><![CDATA[[?&]s=(?P<BITRATE>\d+)]]></regexp>
				<regexp id="url_format" match="FORMAT"><![CDATA[[?&]c=(?P<FORMAT>\w+)]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<multi_regexp id="json_file_colon" content="page" var_prefix="urls_" match="CONTENT">
				<set_var id="bitrate"><regexp id="url_bitrate" content="urls_CONTENT"/></set_var>
				<set_var id="format"><regexp id="url_format" content="urls_CONTENT"/></set_var>
				<best_var id="bitrate" type="numeric_ascending" reset="format">
					<best_var id="format" type="enumeration" order="mp4,webm">
						<set_var id="url"><get_var id="urls_CONTENT"/></set_var>
					</best_var>
				</best_var>
			</multi_regexp>
			<http_download>
				<title><regexp id="meta_ogtitle" content="page"/></title>
				<url><get_var id="url"/></url>
				<extension>.mp4</extension>
			</http_download>
		</script>

		<script id="ceskatelevize.cz" provider="CeskaTelevize.cz">
			<!-- Pozn.: Jen embeddovana videa, vlastni stahovani je primo v YTD -->
			<regexps>
				<regexp id="frame" match="URL"><![CDATA[<iframe\b[^>]*\ssrc="(?P<URL>(?:https?://[^/]+)?/ivysilani/.+?)"]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<multi_regexp id="frame" content="page" var_prefix="frame_" match="URL">
				<nested_download>
					<title><decode_html><regexp id="title" content="page"/></decode_html></title>
					<url><relative_url><url><encode_url><decode_html><get_var id="frame_URL"/></decode_html></encode_url></url><base><get_var id="_movie_id"/></base></relative_url></url>
				</nested_download>
			</multi_regexp>
		</script>

		<script id="cestyksobe.cz" provider="CestyKSobe.cz">
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<nested_download>
				<title><regexp id="h1" content="page"/></title>
				<url><decode_html><regexp id="iframe_src" content="page"/></decode_html></url>
			</nested_download>
		</script>

		<script id="cloudzilla.to" provider="Cloudzilla.to">
			<regexps>
				<regexp id="url" match="URL"><![CDATA[var\s+vurl\s*=\s*"(?P<URL>https?://.+?)"]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<http_download>
				<title><regexp id="div_title" content="page"/></title>
				<url><regexp id="url" content="page"/></url>
			</http_download>
		</script>

		<script id="csfd.cz" provider="CSFD.cz">
			<regexps>
				<regexp id="base_title" match="NAME"><![CDATA[<meta\s+property="og:title"\s+content="(?P<NAME>.+?)(?:\s*\|[^"]*)?"]]></regexp>
				<regexp id="video_list" match="LIST"><![CDATA[(?:<div\s+class="description\s+flag">(?P<NAME>[^<]+)</div>.*?)?\.addClip\s*\(\s*"[^"]*"\s*,\s*\[\s*(?P<LIST>\{.+?\})\s*\]]]></regexp>
				<regexp id="video_info" match="URL"><![CDATA[\{\s*"src"\s*:\s*"(?P<URL>https?:[^"]+)"\s*,\s*"type"\s*:\s*"(?P<TYPE>[^"]*)"\s*,\s*"quality"\s*:\s*"(?P<QUALITY>\d+).*?\}]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="base_title"><regexp id="base_title" content="page"/></set_var>
			<multi_regexp id="video_list" content="page" var_prefix="video_" match="NAME,LIST">
				<set_var id="url"/>
				<best_var reset="vid_QUALITY"/>
				<multi_regexp id="video_info" content="video_LIST" var_prefix="vid_" match="URL,TYPE,QUALITY">
					<best_var id="vid_QUALITY" type="numeric_ascending" reset="vid_TYPE">
						<best_var id="vid_TYPE" type="enumeration" order="video\/mp4,video\/webm">
							<set_var id="url"><decode_js><get_var id="vid_URL"/></decode_js></set_var>
						</best_var>
					</best_var>
				</multi_regexp>
				<http_download>
					<title><get_var id="base_title"/> - <get_var id="video_NAME"/></title>
					<url><get_var id="url"/></url>
				</http_download>
			</multi_regexp>
		</script>

		<script id="dailymail.co.uk" provider="DailyMail.co.uk">
