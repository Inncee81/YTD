<ytd-defs>
	<version>1.41.20140720.0</version>
	<scripts>

		<script id="1tv.ru" provider="1tv.ru">
			<regexps>
				<regexp id="playlist" match="CONTENT"><![CDATA[\bplaylistObj\s*=\s*\[(?P<CONTENT>.+?)\]\s*;]]></regexp>
				<regexp id="movie" match="URL"><![CDATA[\{[^}]*'file'\s*:\s*'(?P<URL>https?://[^']+)'\s*,\s*'title'\s*:\s*'(?P<TITLE>[^']*)'[^}]*\}]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="ansi"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="playlist"><regexp id="playlist" content="page"/></set_var>
			<multi_regexp id="movie" content="playlist" var_prefix="movie_" match="URL,TITLE">
				<http_download>
					<title><get_var id="movie_TITLE"/></title>
					<url><get_var id="movie_URL"/></url>
				</http_download>
			</multi_regexp>
		</script>

		<script id="aktualne.cz" provider="Aktualne.cz">
			<regexps>
				<regexp id="url_bitrate" match="BITRATE"><![CDATA[[?&]s=(?P<BITRATE>\d+)]]></regexp>
				<regexp id="url_format" match="FORMAT"><![CDATA[[?&]c=(?P<FORMAT>\w+)]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<multi_regexp id="json_file_colon" content="page" var_prefix="urls_" match="CONTENT">
				<set_var id="bitrate"><regexp id="url_bitrate" content="urls_CONTENT"/></set_var>
				<set_var id="format"><regexp id="url_format" content="urls_CONTENT"/></set_var>
				<best_var id="bitrate" type="numeric_ascending" reset="format">
					<best_var id="format" type="enumeration" order="mp4,webm">
						<set_var id="url"><get_var id="urls_CONTENT"/></set_var>
					</best_var>
				</best_var>
			</multi_regexp>
			<http_download>
				<title><regexp id="meta_ogtitle" content="page"/></title>
				<url><get_var id="url"/></url>
				<extension>.mp4</extension>
			</http_download>
		</script>

		<script id="cestyksobe.cz" provider="CestyKSobe.cz">
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<nested_download>
				<title><regexp id="h1" content="page"/></title>
				<url><decode_html><regexp id="iframe_src" content="page"/></decode_html></url>
			</nested_download>
		</script>

		<script id="csfd.cz" provider="CSFD.cz">
			<regexps>
				<regexp id="base_title" match="NAME"><![CDATA[<meta\s+property="og:title"\s+content="(?P<NAME>.+?)(?:\s*\|[^"]*)?"]]></regexp>
				<regexp id="video_list" match="LIST"><![CDATA[(?:<div\s+class="description\s+flag">(?P<NAME>[^<]+)</div>.*?)?\.addClip\s*\(\s*"[^"]*"\s*,\s*\[\s*(?P<LIST>\{.+?\})\s*\]]]></regexp>
				<regexp id="video_info" match="URL"><![CDATA[\{\s*"src"\s*:\s*"(?P<URL>https?:[^"]+)"\s*,\s*"type"\s*:\s*"(?P<TYPE>[^"]*)"\s*,\s*"quality"\s*:\s*"(?P<QUALITY>\d+).*?\}]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="base_title"><regexp id="base_title" content="page"/></set_var>
			<multi_regexp id="video_list" content="page" var_prefix="video_" match="NAME,LIST">
				<set_var id="url"/>
				<best_var reset="vid_QUALITY"/>
				<multi_regexp id="video_info" content="video_LIST" var_prefix="vid_" match="URL,TYPE,QUALITY">
					<best_var id="vid_QUALITY" type="numeric_ascending" reset="vid_TYPE">
						<best_var id="vid_TYPE" type="enumeration" order="video\/mp4,video\/webm">
							<set_var id="url"><decode_js><get_var id="vid_URL"/></decode_js></set_var>
						</best_var>
					</best_var>
				</multi_regexp>
				<http_download>
					<title><get_var id="base_title"/> - <get_var id="video_NAME"/></title>
					<url><get_var id="url"/></url>
				</http_download>
			</multi_regexp>
		</script>

		<script id="dailymail.co.uk" provider="DailyMail.co.uk">
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<http_download>
				<title><regexp id="meta_articletag" content="page"/></title>
				<url><regexp id="meta_ogvideo" content="page"/></url>
			</http_download>
		</script>

		<script id="facebook.com" provider="Facebook.com">
			<regexps>
				<regexp id="item" match="CONTENT"><![CDATA[swf\s*=\s*new\s+SWFObject\s*\((?P<CONTENT>.+?)\breturn\s+swf\s*;]]></regexp>
				<regexp id="params" match="CONTENT"><![CDATA[\[\s*\[\s*"params"\s*,\s*"(?P<CONTENT>.+?)"]]></regexp>
				<regexp id="url" match="URL"><![CDATA["hd_src"\s*:\s*"(?P<URL>https?:.+?)"]]></regexp>
				<regexp id="title" match="TITLE"><![CDATA[<h2\s+class="uiHeaderTitle">(?P<TITLE>.+?)</h2>]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="item"><regexp id="item" content="page"/></set_var>
			<set_var id="params"><decode_url><decode_js><regexp id="params" content="item"/></decode_js></decode_url></set_var>
			<http_download>
				<title><regexp id="title" content="page"/></title>
				<url><decode_js><regexp id="url" content="params"/></decode_js></url>
			</http_download>
		</script>

		<script id="mc2.sk" provider="MC2.sk">
			<regexps>
				<regexp id="server" match="URL"><![CDATA[\.addVariable\s*\(\s*"node"\s*,\s*"(?P<URL>rtmpt?e?://[^"/]+)]]></regexp>
				<regexp id="playpath" match="URL"><![CDATA[\$\s*\(\s*"hd"\s*\)\s*\.\s*setProperty\s*\(\s*"rel"\s*,\s*"(?P<URL>[^"]+)"]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<rtmp_download>
				<title><regexp id="meta_ogtitle" content="page"/></title>
				<url><regexp id="server" content="page"/>:80/</url>
				<playpath><regexp id="playpath" content="page"/></playpath>
				<!-- lze pouzivat take rtmp_app, swf_url, tc_url, page_url, swf_vfy, flashver, live, realtime -->
			</rtmp_download>
		</script>

		<script id="rtl.be" provider="RTL.be">
			<regexps>
				<regexp id="idvid" match="ID"><![CDATA[\bidvid\s*=\s*'(?P<ID>\d+)']]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="idvid"><regexp id="idvid" content="page"/></set_var>
			<set_var id="info"><download_page encoding="xml"><url>http://www.rtl.be/videos/GetFlashParams.aspx?sPublisherKey=&amp;id=<get_var id="idvid"/>&amp;bFlash=1&amp;bEmbed=0&amp;bAutoStart=1&amp;bAutoMute=0&amp;sDummyPath=/videos&amp;ts=<timestamp type="unix"/></url></download_page></set_var>
			<http_download>
				<title><get_xml_var id="info" path="VPO/Title"/></title>
				<url><get_xml_var id="info" path="VPO/URL"/></url>
			</http_download>
		</script>

		<script id="super.cz" provider="Super.cz">
			<regexps>
				<regexp id="url" match="URL"><![CDATA[(?:&amp;|&|^:)cdn2(?P<QUALITY>HD|HQ|LQ)=(?P<URL>[^&]+)]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url><get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="flashvars"><regexp id="param_name_flashvars" content="page"/></set_var>
			<multi_regexp id="url" content="flashvars" var_prefix="urls_" match="QUALITY,URL">
				<best_var id="urls_QUALITY" type="enumeration" order="HD,HQ,LQ">
					<set_var id="url"><get_var id="urls_URL"/></set_var>
				</best_var>
			</multi_regexp>
			<http_download>
				<title><regexp id="title" content="page"/></title>
				<url><get_var id="url"/></url>
			</http_download>
		</script>

		<script id="vimeo.com" provider="Vimeo.com">
			<regexps>
				<regexp id="info_url" match="URL"><![CDATA[\sdata-config-url="(?P<URL>.+?)"]]></regexp>
				<regexp id="video_list" match="CONTENT"><![CDATA["request"\s*:\s*\{\s*"files"\s*:\s*\{\s*"h264"\s*:\s*\{(?P<CONTENT>.+?\})\s*\}]]></regexp>
				<regexp id="video_item"><![CDATA["[^"]+"\s*:\s*\{[^}]*"url"\s*:\s*"(?P<URL>https?://[^"]+)"\s*,[^}]*"bitrate"\s*:\s*(?P<BITRATE>\d+)[^}]*\}]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf-8"><url>http://vimeo.com/<get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="info"><download_page encoding="utf-8"><url><decode_html><regexp id="info_url" content="page"/></decode_html></url></download_page></set_var>
			<set_var id="list"><regexp id="video_list" content="info"/></set_var>
			<multi_regexp id="video_item" content="list" var_prefix="urls_" match="BITRATE,URL">
				<best_var id="urls_BITRATE" type="numeric_ascending">
					<set_var id="url"><get_var id="urls_URL"/></set_var>
				</best_var>
			</multi_regexp>
			<http_download>
				<title><regexp id="meta_ogtitle" content="page"/></title>
				<url><get_var id="url"/></url>
			</http_download>
		</script>

		<!-- ***** Adult ******************************************* -->
		<adult>
		
		<script id="123video.nl" provider="123video.nl">
			<regexps>
				<regexp id="title" match="TITLE"><![CDATA[<div\s+class="box">\s*<div\s+class="top"[^>]*>\s*<div\s+class="top"[^>]*>\s*(?P<TITLE>.*?)\s*</div>]]></regexp>
			</regexps>
			<set_var id="page"><download_page encoding="utf8"><url>http://www.123video.nl/playvideos.asp?MovieID=<get_var id="_movie_id"/></url></download_page></set_var>
			<set_var id="server"><download_page encoding="xml">
				<url>http://www.123video.nl/initialize_player_v3.asp</url>
				<post_data><![CDATA[<movie><id>]]><get_var id="_movie_id"/><![CDATA[</id><memberid>0</memberid><cnt>1</cnt><nocache>32</nocache></movie>]]></post_data>
				<xml_path></xml_path>
				<xml_attr>MediaIP</xml_attr>
			</download_page></set_var>
			<http_download>
				<title><regexp id="title" content="page"/></title>
				<url>http://<get_var id="server"/>/<copy start="0" length="-3"><get_var id="_movie_id"/></copy>/<get_var id="_movie_id"/>.flv></url>
			</http_download>
		</script>

		</adult>
	</scripts>
	<regexps>
		<regexp id="title" match="NAME"><![CDATA[<title>\s*(?P<NAME>.*?)\s*</title>]]></regexp>
		<regexp id="h1" match="NAME"><![CDATA[<h1[^>]*>\s*(?P<NAME>.*?)\s*</h1>]]></regexp>
		<regexp id="meta_ogtitle" match="NAME"><![CDATA[<meta\s+(name|property)=(?P<QUOTE1>['"])og:title(?P=QUOTE1)\s+(content|value)=(?P<QUOTE2>['"])(?P<NAME>.*?)(?P=QUOTE2)]]></regexp>
		<regexp id="meta_ogvideo" match="NAME"><![CDATA[<meta\s+(name|property)=(?P<QUOTE1>['"])og:video(?P=QUOTE1)\s+(content|value)=(?P<QUOTE2>['"])(?P<NAME>.*?)(?P=QUOTE2)]]></regexp>
		<regexp id="meta_articletag" match="NAME"><![CDATA[<meta\s+(name|property)=(?P<QUOTE1>['"])article:tag(?P=QUOTE1)\s+(content|value)=(?P<QUOTE2>['"])(?P<NAME>.*?)(?P=QUOTE2)]]></regexp>
		<regexp id="json_file_colon" match="CONTENT"><![CDATA[\bfile\s*:\s*(?P<QUOTE>['"])(?P<CONTENT>.*?)(?P=QUOTE)]]></regexp>
		<regexp id="iframe_src" match="CONTENT"><![CDATA[<iframe\b[^>]*\ssrc=(?P<QUOTE>['"])(?P<CONTENT>.*?)(?P=QUOTE)]]></regexp>
		<regexp id="param_name_flashvars" match="CONTENT"><![CDATA[<param\s+name=(?P<QUOTE1>['"])flashvars(?P=QUOTE1)\s+value=(?P<QUOTE2>['"])(?P<CONTENT>.+?)(?P=QUOTE2)]]></regexp>
	</regexps>
	<urls>
		<url script="1tv.ru"><![CDATA[1tv\.ru/.+$]]></url>
		<url script="aktualne.cz"><![CDATA[aktualne(?:\.centrum)?\.cz/.+$]]></url>
		<url script="cestyksobe.cz"><![CDATA[cestyksobe\.cz/.+$]]></url>
		<url script="csfd.cz"><![CDATA[csfd\.cz/.+$]]></url>
		<url script="dailymail.co.uk"><![CDATA[dailymail\.co\.uk/.+$]]></url>
		<url script="facebook.com"><![CDATA[facebook\.com/.+$]]></url>
		<url script="mc2.sk"><![CDATA[mc2\.sk/.+$]]></url>
		<url script="rtl.be"><![CDATA[rtl\.be/.+$]]></url>
		<url script="super.cz"><![CDATA[super\.cz/.+$]]></url>
		<url script="vimeo.com" subexpression="ID"><![CDATA[player\.vimeo\.com/video/(?P<ID>\d+)]]></url>
		<url script="vimeo.com" subexpression="ID"><![CDATA[vimeo\.com/(?P<ID>\d+)]]></url>
		<!-- ***** Adult ******************************************* -->
		<adult>
		<url script="123video.nl" subexpression="ID"><![CDATA[123video\.nl/playvideos\.asp\?([^&]*&)*?MovieID=(?P<ID>\d+)]]></url>
		</adult>
	</urls>
</ytd-defs>
